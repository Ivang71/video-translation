    "googleapis": "^128.0.0",
    "mongodb": "^6.3.0",
    "node-fetch": "^3.3.2",
    "playwright": "^1.39.0",
    "python-shell": "^5.0.0",
    "winston": "^3.11.0",
    "youtubei.js": "^7.0.0",
    "ytdl-core": "^4.11.5"
  },
  "devDependencies": {
    "@types/node": "^20.8.9",
    "nodemon": "^3.0.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  }
